<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.howaboutliving.dao.TestDao">
	<select id="getList" resultType="com.howaboutliving.dto.Test">
		SELECT * FROM test
	</select>
	
	<insert id="insert" parameterType="com.howaboutliving.dto.Test">
		INSERT into test values
		(#{SEQ}, #{ITEMID})
	</insert>
	
	<insert id="insertList" parameterType="java.util.List">
		INSERT into test
		(SEQ, ITEMID) values
		<foreach item="item" index="index" collection="list" separator=",">
		(#{item.SEQ}, #{item.ITEMID})
		</foreach>
	</insert>
</mapper>